    "info": {
        "title": "Pixel Game Server API",
        "description": "\u934d\u5fd5\u790c\u5a13\u544a\u5799\u93c8\u5d85\u59df\u9363\u02cbPI\u93c2\u56e8\u3002 - \u9356\u546d\u60c8\u9422\u3126\u57db\u7481\u3088\u7609\u9286\u4f7a\u6ae5\u8930\u66df\u655e\u9350\u5c80\u74d1\u9354\u71bb\u5158",
        "version": "1.0.0",
        "contact": {}
    },
    "tags": [
        {
            "name": "auth",
            "description": "\u9422\u3126\u57db\u7481\u3088\u7609\u9429\u7a3f\u53e7\u93ba\u30e5\u5f5b"
        }
    ],
    "servers": [],
    "components": {
        "securitySchemes": {
            "JWT-auth": {
                "scheme": "bearer",
                "bearerFormat": "JWT",
                "type": "http",
                "name": "JWT",
                "description": "\u7487\u75af\u7ded\u934f\udca5JWT token",
                "in": "header"
            }
        },
        "schemas": {
            "LoginDto": {
                "type": "object",
                "properties": {
                    "identifier": {
                        "type": "string",
                        "description": "\u9427\u8bf2\u7d8d\u93cd\u56ea\u7611\u7ed7\ufe3c\u7d1d\u93c0\udcaf\u93b8\u4f7a\u6564\u93b4\u5cf0\u6095\u9286\u4f80\u5056\u7ee0\u8fa8\u57a8\u93b5\u5b2b\u6e80\u9359\udcb7",
                        "example": "testuser",
                        "minLength": 1,
                        "maxLength": 100
                    },
                    "password": {
                        "type": "string",
                        "description": "\u9422\u3126\u57db\u7035\u55d9\u721c",
                        "example": "password123",
                        "minLength": 1,
                        "maxLength": 128
                    }
                },
                "required": [
                    "identifier",
                    "password"
                ]
            },
            "UserInfoDto": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "description": "\u9422\u3126\u57dbID",
                        "example": "1"
                    },
                    "username": {
                        "type": "string",
                        "description": "\u9422\u3126\u57db\u935a\udc8d",
                        "example": "testuser"
                    },
                    "nickname": {
                        "type": "string",
                        "description": "\u9422\u3126\u57db\u93c4\u7535\u041e",
                        "example": "\u5a34\u5b2d\u762f\u9422\u3126\u57db"
                    },
                    "email": {
                        "type": "string",
                        "description": "\u95ad\udcae\u7ee0\u535e\u6e74\u9367\udc80",
                        "example": "test@example.com"
                    },
                    "phone": {
                        "type": "string",
                        "description": "\u93b5\u5b2b\u6e80\u9359\u98ce\u721c",
                        "example": "+8613800138000"
                    },
                    "avatar_url": {
                        "type": "string",
                        "description": "\u6fb6\u6751\u511aURL",
                        "example": "https://example.com/avatar.jpg"
                    },
                    "role": {
                        "type": "number",
                        "description": "\u9422\u3126\u57db\u7459\u6395\u58ca",
                        "example": 1
                    },
                    "created_at": {
                        "format": "date-time",
                        "type": "string",
                        "description": "\u9352\u6d98\u7f13\u93c3\u5815\u68ff",
                        "example": "2025-12-17T10:00:00.000Z"
                    }
                },
                "required": [
                    "id",
                    "username",
                    "nickname",
                    "role",
                    "created_at"
                ]
            },
            "LoginResponseDataDto": {
                "type": "object",
                "properties": {
                    "user": {
                        "description": "\u9422\u3126\u57db\u6dc7\u2103\u4f05",
                        "allOf": [
                            {
                                "$ref": "#/components/schemas/UserInfoDto"
                            }
                        ]
                    },
                    "access_token": {
                        "type": "string",
                        "description": "\u7481\u5757\u68f6\u6d60\u3087\u589d",
                        "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
                    },
                    "refresh_token": {
                        "type": "string",
                        "description": "\u9352\u950b\u67ca\u6d60\u3087\u589d",
                        "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
                    },
                    "is_new_user": {
                        "type": "boolean",
                        "description": "\u93c4\udcaf\u935a\ufe3f\u8d1f\u93c2\u626e\u6564\u93b4\udcb7",
                        "example": false
                    },
                    "message": {
                        "type": "string",
                        "description": "\u935d\u5d85\u7c32\u5a11\u581f\u4f05",
                        "example": "\u9427\u8bf2\u7d8d\u93b4\u612c\u59db"
                    }
                },
                "required": [
                    "user",
                    "access_token",
                    "message"
                ]
            },
            "LoginResponseDto": {
                "type": "object",
                "properties": {
                    "success": {
                        "type": "boolean",
                        "description": "\u7487\u950b\u7730\u93c4\udcaf\u935a\ufe3d\u579a\u9354\udc9f",
                        "example": true
                    },
                    "data": {
                        "description": "\u935d\u5d85\u7c32\u93c1\u7248\u5d41",
                        "allOf": [
                            {
                                "$ref": "#/components/schemas/LoginResponseDataDto"
                            }
                        ]
                    },
                    "message": {
                        "type": "string",
                        "description": "\u935d\u5d85\u7c32\u5a11\u581f\u4f05",
                        "example": "\u9427\u8bf2\u7d8d\u93b4\u612c\u59db"
                    },
                    "error_code": {
                        "type": "string",
                        "description": "\u95bf\u6b12\udcaf\udcaf\u6d60\uff47\u721c",
                        "example": "LOGIN_FAILED"
                    }
                },
                "required": [
                    "success",
                    "message"
                ]
            },
            "RegisterDto": {
                "type": "object",
                "properties": {
                    "username": {
                        "type": "string",
                        "description": "\u9422\u3126\u57db\u935a\u5d8f\u7d1d\u9359\udcaa\u9473\u85c9\u5bd8\u935a\udcab\u701b\u6941\u761d\u9286\u4f79\u669f\u701b\u6940\u62f0\u6d93\u5b2a\u579d\u7efe\udcbf",
                        "example": "testuser",
                        "minLength": 1,
                        "maxLength": 50,
                        "pattern": "^[a-zA-Z0-9_]+$"
                    },
